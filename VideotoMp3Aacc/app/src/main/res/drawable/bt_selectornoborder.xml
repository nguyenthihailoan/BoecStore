<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:state_pressed="true"
        >
        <shape>
            <corners android:radius="@dimen/s10dp" />
            <solid android:color="#dc04006c" />
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="@dimen/s1dp" android:color="@color/colorAccent"/>
            <corners android:radius="@dimen/s10dp" />
            <solid android:color="#f401001d" />
        </shape>
    </item>
</selector>