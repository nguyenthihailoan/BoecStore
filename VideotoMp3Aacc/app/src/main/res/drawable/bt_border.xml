<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <stroke android:width="@dimen/s1dp" android:color="#ff0000" />
            <corners android:radius="@dimen/s5dp" />
            <solid android:color="#00111111" />
        </shape>
    </item>
    <item>
        <shape>
            <stroke android:width="@dimen/s1dp" android:color="@color/colorAccent" />
            <corners android:radius="@dimen/s5dp" />
            <solid android:color="#00111111" />
        </shape>
    </item>
</selector>